{
  "name": "@saberhq/sail",
  "version": "2.4.4",
  "description": "Account caching and batched loading for React-based Solana applications.",
  "keywords": [
    "solana",
    "blockchain",
    "react"
  ],
  "author": "Ian Macalinao <ian@saber.so>",
  "homepage": "https://sail.saber.so",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/saber-hq/sail/issues",
    "email": "ian@saber.so"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saber-hq/sail.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "module": "./esm/index.js",
      "import": "./esm/index.mjs",
      "default": "./index.js"
    },
    "./*": {
      "types": "./*.d.ts",
      "module": "./esm/*.js",
      "import": "./esm/*.mjs",
      "default": "./*.js"
    }
  },
  "scripts": {
    "build": "run-p build:es5 build:esm",
    "build:es5": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "lint": "eslint . --cache",
    "lint:ci": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "docs:generate": "typedoc --excludePrivate --includeVersion --out site/ts/ src/index.ts"
  },
  "devDependencies": {
    "@saberhq/eslint-config-react": "^1.3.1",
    "@types/lodash.zip": "^4.2.6",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "bn.js": "^5.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.13.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "run-p": "^0.0.0",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@saberhq/solana-contrib": "^1.3.1",
    "@saberhq/token-utils": "^1.3.1",
    "@saberhq/use-solana": "^1.3.1",
    "@solana/web3.js": "^1.29.1",
    "dataloader": "^2.0.0",
    "eventemitter3": "^4.0.7",
    "lodash.zip": "^4.2.0",
    "tiny-invariant": "^1.1.0",
    "tslib": "^2.3.1",
    "unstated-next": "^1.1.0",
    "use-debounce": "^7.0.0"
  },
  "peerDependencies": {
    "bn.js": "^5.2.0",
    "jsbi": "*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "packageManager": "yarn@3.0.2"
}
