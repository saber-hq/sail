{
  "name": "@saberhq/sail",
  "version": "2.4.8",
  "description": "Account caching and batched loading for React-based Solana applications.",
  "keywords": [
    "solana",
    "blockchain",
    "react"
  ],
  "author": "Ian Macalinao <ian@saber.so>",
  "homepage": "https://sail.saber.so",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/saber-hq/sail/issues",
    "email": "ian@saber.so"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saber-hq/sail.git"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --cache",
    "lint:ci": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "docs:generate": "typedoc --excludePrivate --includeVersion --out site/ts/ src/index.ts",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@saberhq/eslint-config-react": "^1.3.5",
    "@types/lodash.zip": "^4.2.6",
    "@types/react": "^17.0.25",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "bn.js": "^5.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@saberhq/solana-contrib": "^1.3.5",
    "@saberhq/token-utils": "^1.3.5",
    "@saberhq/use-solana": "^1.3.5",
    "@solana/web3.js": "^1.29.2",
    "dataloader": "^2.0.0",
    "eventemitter3": "^4.0.7",
    "lodash.zip": "^4.2.0",
    "tiny-invariant": "^1.1.0",
    "tslib": "^2.3.1",
    "unstated-next": "^1.1.0",
    "use-debounce": "^7.0.0"
  },
  "peerDependencies": {
    "bn.js": "^5.2.0",
    "jsbi": "*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "packageManager": "yarn@3.0.2",
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{json,js,jsx,html,css}": "prettier --write"
  }
}
